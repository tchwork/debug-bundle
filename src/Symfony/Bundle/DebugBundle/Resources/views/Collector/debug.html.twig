{% extends '@WebProfiler/Profiler/layout.html.twig' %}

{% block toolbar %}
    {% set dumps_count = collector.dumpsCount %}

    {% if dumps_count %}
        {% set icon %}
            <img alt="debug()" src="data:image/png;base64," />
            <span class="sf-toolbar-status{% if dumps_count > 0 %} sf-toolbar-status-yellow{% endif %}">{{ dumps_count }}</span>
        {% endset %}

        {% set text %}
            <div class="sf-toolbar-info-piece">
                <b>debug()</b>
            </div>
            {% for dump in collector.dumpsExcerpts %}
            <div class="sf-toolbar-info-piece">
                {{ dump.dataExcerpt }}
            </div>
            {% endfor %}
        {% endset %}

        {% include '@WebProfiler/Profiler/toolbar_item.html.twig' with { 'link': true } %}
    {% endif %}
{% endblock %}

{% block menu %}
    <span class="label">
        <span class="icon">{% spaceless %}
        <img alt="debug()" src="data:image/png;base64," />
            {% endspaceless %}</span>
        <strong>debug()</strong>
        <span class="count">
            <span>{{ collector.dumpsCount }}</span>
        </span>
    </span>
{% endblock %}

{% block panel %}
    <h2>debug()</h2>

    <link rel="stylesheet" href="{{ asset('bundles/debug/css/debug.css') }}" />
    <script src="{{ asset('bundles/debug/js/debug.js') }}"></script>

    <ul id="var_debug" class="alt">
        {% for dump in collector.dumps(true) %}
        <li class="dump sf-reset">
            in {% if dump.file %}<abbr title="{{ dump.file }}">{{ dump.name }}</abbr>{% else %}{{ dump.name }}{% endif %}
            line {{ dump.line }}:
            <a onclick="arrayToggle(this)"> âŠž </a><span class="key"></span>
            <span class="array-compact">
            {% if dump.fileExcerpt %}{{ dump.fileExcerpt|raw }}{% else %}{{ dump.file|file_excerpt(dump.line) }}{% endif %}
            </span>

            <pre class="event"><script>document.write(htmlizeEvent({{ dump.data|raw }}))</script></pre>
        </li>
        {% endfor %}
    </ul>
{% endblock %}
